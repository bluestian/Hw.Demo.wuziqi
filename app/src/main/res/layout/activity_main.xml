<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:wzq="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="kf.qf.com.hwdemowuziqi.MainActivity">

   <kf.qf.com.hwdemowuziqi.CustomView
       android:id="@+id/id_wuziqi"
       android:layout_centerInParent="true"
       android:layout_width="match_parent"
       android:layout_height="match_parent"
       wzq:panel_background="@drawable/bg"
       wzq:panel_line_color="#C00"
       wzq:white_piece_img="@drawable/stone_w2"
       wzq:black_piece_img="@drawable/stone_b1"
       wzq:max_count_line="10"
       wzq:max_win_count_piece="6"
       />
   <Button
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:onClick="btn"
       android:text="获取信息"/>
</RelativeLayout>
